{
  "name": "wadr",
  "version": "1.0.0",
  "description": "Wappalyzer domain report tool.",
  "main": "index.js",
  "scripts": {
    "hello": "functions-framework --target=helloWorld",
    "start": "npm-watch startWatch",
    "inspect": "npm-watch inspectWatch",
    "startWatch": "npx @google-cloud/functions-framework --target=entry",
    "inspectWatch": "npx --node-arg=--inspect @google-cloud/functions-framework --target=entry",
    "deploy": "gcloud functions deploy almanac --project pantheon-sales --entry-point entry --runtime nodejs14 --trigger-http --allow-unauthenticated"
  },
  "watch": {
    "startWatch": "*.js",
    "inspectWatch": "*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyletaylored/wadr.git"
  },
  "keywords": [
    "wappalyzer",
    "report",
    "google",
    "sheets",
    "app",
    "script"
  ],
  "author": "Kyle Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyletaylored/wadr/issues"
  },
  "homepage": "https://github.com/kyletaylored/wadr#readme",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.9.0",
    "cli-progress": "^3.9.0",
    "dotenv": "^10.0.0",
    "gcp-metadata": "^4.3.1",
    "google-auth-library": "^7.10.1",
    "normalize-url": "^4.5.1",
    "npm-watch": "^0.10.0",
    "pino": "^7.0.5",
    "pino-http": "^5.8.0",
    "securitytrails-js": "git+https://github.com/kyletaylored/securitytrails-js.git#update-stapi-endpoints",
    "user-agents": "^1.0.817",
    "wappalyzer": "^6.8.19"
  }
}
